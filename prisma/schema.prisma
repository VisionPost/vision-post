generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id                  String    @id  @default(uuid())
  name                String?
  email               String    @unique
  image               String?
  githubUsername      String    @unique
  provider            String
  providerAccountId   String    @unique
  access_token        String?
  x_userName          String?
}
